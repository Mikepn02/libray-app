generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id        String   @id @default(cuid())
  firstName String
  lastName  String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Book {
  id              Int      @id @default(autoincrement())
  name            String
  author          String
  publisher       String
  publicationYear Int
  subject         String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
